{
  "name": "p5-notebook",
  "version": "0.1.0",
  "description": "A minimal Jupyter Notebook UI for p5.js kernels",
  "license": "BSD-3-Clause",
  "author": "Jeremy Tuloup",
  "keywords": [
    "p5",
    "jupyter",
    "notebook"
  ],
  "homepage": "https://github.com/jtpio/p5-notebook",
  "bugs": {
    "url": "https://github.com/jtpio/p5-notebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/p5-notebook.git"
  },
  "scripts": {
    "build": "tsc && webpack",
    "build:prod": "yarn run clean && tsc && webpack --mode=production",
    "stage": "node stage.js",
    "clean": "rimraf dist && rimraf build && rimraf tsconfig.tsbuildinfo",
    "deploy": "yarn run build:prod && yarn run stage",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "prettier:check": "prettier --list-different \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "watch:ts": "tsc -w --listEmittedFiles",
    "watch:webpack": "webpack --watch",
    "watch": "npm-run-all --parallel watch:ts watch:webpack"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/apputils": "^2.0.2",
    "@jupyterlab/apputils-extension": "^2.0.2",
    "@jupyterlab/cells": "^2.0.2",
    "@jupyterlab/codemirror": "^2.0.2",
    "@jupyterlab/completer": "^2.0.2",
    "@jupyterlab/coreutils": "^4.0.2",
    "@jupyterlab/docmanager": "^2.0.2",
    "@jupyterlab/docregistry": "^2.0.2",
    "@jupyterlab/documentsearch": "^2.0.2",
    "@jupyterlab/mainmenu": "^2.0.2",
    "@jupyterlab/mathjax2": "^2.0.1",
    "@jupyterlab/nbformat": "^2.0.1",
    "@jupyterlab/notebook": "^2.0.2",
    "@jupyterlab/observables": "^3.0.1",
    "@jupyterlab/rendermime": "^2.0.2",
    "@jupyterlab/services": "^5.0.2",
    "@jupyterlab/settingregistry": "^2.0.1",
    "@jupyterlab/shortcuts-extension": "^2.0.2",
    "@jupyterlab/statedb": "^2.0.1",
    "@jupyterlab/theme-dark-extension": "^2.0.2",
    "@jupyterlab/theme-light-extension": "^2.0.2",
    "@jupyterlab/ui-components": "^2.0.2",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/commands": "^1.10.1",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/disposable": "^1.3.5",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.1",
    "es6-promise": "~4.2.8",
    "jupyterlab-cell-flash": "^0.2.2",
    "jupyterlab-theme-toggle": "^0.5.0",
    "jupyterlab-topbar-extension": "^0.5.0",
    "mock-socket": "^9.0.3",
    "p5": "^1.0.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "strip-json-comments": "^3.0.1"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.76",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "css-loader": "~3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "fs-extra": "^9.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "mini-css-extract-plugin": "~0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "rimraf": "~3.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "~1.0.1",
    "svg-url-loader": "~3.0.3",
    "typescript": "~3.8.3",
    "url-loader": "~3.0.0",
    "watch": "~1.0.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "react": "~16.9.0",
    "react-dom": "~16.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
